{
  "name": "full-stack-boilerplate",
  "version": "1.0.0",
  "description": "Because this is a pain to do every single time.",
  "main": "server/index.js",
  "scripts": {
    "build": "webpack",
    "build-watch": "webpack -w",
    "seed": "node db/seed.js",
    "start": "nodemon server/index.js",
    "test": "cross-env NODE_ENV=testing mocha --compilers js:babel-register browser/**/*.test.js browser/**/*.test.jsx db/**/*.test.js server/**/*.test.js",
    "test-watch": "cross-env NODE_ENV=testing mocha --compilers js:babel-register --watch browser/**/*.test.js browser/**/*.test.jsx db/**/*.test.js server/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bethqiang/full-stack-boilerplate.git"
  },
  "keywords": [
    "boilerplate",
    "react",
    "redux",
    "immutable",
    "express",
    "node.js",
    "postgresql",
    "sequelize"
  ],
  "author": "Beth Qiang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bethqiang/full-stack-boilerplate/issues"
  },
  "homepage": "https://github.com/bethqiang/full-stack-boilerplate#readme",
  "dependencies": {
    "axios": "^0.15.3",
    "babel": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.16.1",
    "chalk": "^1.1.3",
    "cookie-session": "^2.0.0-beta.1",
    "cross-env": "^3.1.4",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "immutable": "^3.8.1",
    "nodemon": "^1.11.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pg": "^6.1.2",
    "pg-native": "^1.10.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.1",
    "redux-thunk": "^2.2.0",
